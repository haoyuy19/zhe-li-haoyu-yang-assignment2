{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
        
    },
    "parser": "babel-eslint",
    "extends": [
        
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        //"parser": "babel-eslint",
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        //"parser": "babel-eslint",
        "prettier/prettier": "error",
        "no-unused-vars": "warn"     
    }
    
}